      </ShareProperties>

      <ExposedProperties>
        <Property name="SelectInputVectors"  exposed_name="SelectLICVectors" />
        <Property name="NumberOfSteps" exposed_name="LICNumberOfSteps" />
        <Property name="StepSize" exposed_name="LICStepSize" />
        <Property name="LICIntensity" />
        <Property name="UseLICForLOD" />
      </ExposedProperties>
    </SubProxy>

    <SubProxy>
      <Proxy name="IlluminatedLinesRepresentation"
        proxygroup="representations" proxyname="IlluminatedLinesRepresentation" />
      <ShareProperties subproxy="SurfaceRepresentation">
        <Exception name="Input" />
        <Exception name="Visibility" />
      </ShareProperties>
    </SubProxy>

    <!-- End of UnstructuredGridRepresentation(Extension) -->
  </Extension>

  <Extension name="UniformGridRepresentation" group="representations">
    <!-- This is added to the "UniformGridRepresentation" proxy xml defined in the
         standard ParaView servermanager xml 
    -->
    <Option representation="101" subproxy="SurfaceLICRepresentation" value="2"
            text="Surface LIC" />
    <Option representation="102" subproxy="IlluminatedLinesRepresentation" value="1"
            text="Illuminated Wireframe" />

    <SubProxy>
      <Proxy name="SurfaceLICRepresentation"
