      </ShareProperties>

      <ExposedProperties>
        <Property name="SelectInputVectors"  exposed_name="SelectLICVectors" />
        <Property name="NumberOfSteps" exposed_name="LICNumberOfSteps" />
        <Property name="StepSize" exposed_name="LICStepSize" />
        <Property name="LICIntensity" />
